@using DemoProject.Models.Tag
@using DemoProject.Common
@model TagListVM
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


@if (Model != null)
{
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>No</th>
                <th>Name</th>
                <th>DisplayName</th>
            </tr>
        </thead>
        <tbody>
            @for(var i = 0; i < Model.PaginatedTag.Count; i++)
            {
                var item = Model.PaginatedTag[i];
                <tr>
                    <td></td>
                    <td>@item.Name</td>
                    <td>@item.DisplayName</td>
                </tr>
            }
        </tbody>
    </table>
    {
        IPaging paging = new PaginatedList<TagListVM>();
    }
}
else
{

}